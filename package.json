{
  "name": "switch-language",
  "version": "0.0.0",
  "description": "Switch web page language browser extension",
  "repository": "maxmilton/switch-language",
  "homepage": "https://github.com/maxmilton/switch-language",
  "author": "Max Milton <max@happyhackers.xyz>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production bun build.ts",
    "dev": "NODE_ENV=development bun build.ts",
    "lint": "bun lint:fmt && bun lint:css && bun lint:js && bun lint:ts",
    "lint:css": "stylelint '**/*.{css,xcss}'",
    "lint:fmt": "biome check",
    "lint:js": "eslint",
    "lint:ts": "tsc --noEmit",
    "test": "bun test",
    "test:e2e": "playwright test",
    "test:e2e:debug": "bun test:e2e --debug",
    "test:e2e:ui": "bun test:e2e --ui",
    "zip": "./zip.sh"
  },
  "overrides": {
    "lightningcss": "1.23.0"
  },
  "dependencies": {
    "@ekscss/framework": "0.0.48",
    "stage1": "0.8.0-next.10",
    "trackx": "0.6.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@eslint/compat": "1.1.0",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.6.0",
    "@maxmilton/stylelint-config": "0.0.15",
    "@playwright/test": "1.45.0",
    "@types/bun": "1.1.6",
    "@types/chrome": "0.0.268",
    "@types/csso": "5.0.4",
    "@types/eslint__eslintrc": "2.1.1",
    "@types/eslint__js": "8.42.3",
    "csso": "5.0.5",
    "ekscss": "0.0.17",
    "eslint": "9.6.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-unicorn": "54.0.0",
    "happy-dom": "14.12.3",
    "lightningcss": "1.25.1",
    "purgecss": "6.0.0",
    "stylelint": "16.6.1",
    "stylelint-config-standard": "36.0.1",
    "terser": "5.31.1",
    "typescript": "5.5.3",
    "typescript-eslint": "7.15.0"
  }
}
